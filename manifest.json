{
	"manifest_version": 2,
	"name": "CLS (Clear Local Storage)",
	"version": "1.0",
	"description": "An extension to clear local storage",
	"background": {
		"scripts": [
			"clear.js"
		]
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"48": "48.png"
		}
	},
	"permissions": [
		"storage",
		"activeTab",
		"tabs"
	],
	"icons": {
		"48": "48.png"
	}
}